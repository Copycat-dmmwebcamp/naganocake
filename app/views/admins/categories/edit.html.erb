<h2>ジャンル編集</h2>

<!-- updateのurl -->
<%= form_for(@category, url:admins_category_path) do |f| %>
	ジャンル
    <%= f.text_field :name %>
    <%= f.radio_button :category_status, :true %>
    <%= f.label :category_status_true, "有効" %>
    <%= f.radio_button :category_status, :false %>
    <%= f.label :category_status_false, "無効" %>
    <%= f.submit '変更を保存する' %>
<% end %>